{
  "name": "discord-bot-pro",
  "version": "0.2.0",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/src/index.js",
    "register:commands": "node dist/scripts/register-commands.js",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:register": "cross-env NODE_ENV=development tsx scripts/register-commands.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts --fix",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.40.0"
  }
}
